{
    "compilerOptions": {
        "outDir": "./built/",
        "sourceMap": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",

        "experimentalDecorators": true,

        "strict": true,
        "noUnusedLocals": true,
        // "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true
     },
    "include": [
        "./src/**/*"
    ]
}
